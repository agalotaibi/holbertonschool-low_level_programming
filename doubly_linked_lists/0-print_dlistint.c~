#include <stdlib.h>
#include "lists.h"

/**
* print_dlistint - prints all the elements of a dlistint_t
* @h: A pointer to the head of the list.
* Return: The number of nodes.
*/


size_t print_dlistint(const dlistint_t *h){
int count = 0;
const dlistint_t *temp = h;
while (temp != NULL)
{

printf("%d\n", temp->n);


count++;
temp = temp->next;
}

return (count);



}
